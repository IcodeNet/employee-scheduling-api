{
  "name": "employee-scheduling-api",
  "version": "0.0.0",
  "description": "An API component for Employee Scheduling application.",
  "author": {
    "name": "Martin Micunda",
    "url": "http://martinmicunda.com"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "dev": "nodemon index -w 'lib/**/*' --ext 'js json'",
    "debug": "nodemon index --debug -w 'lib/**/*' --ext 'js json'",
    "lint": "jshint lib/**/*.js",
    "audit": "nsp audit-shrinkwrap && nsp audit-package",
    "missing": "npm ls --depth 1",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "pretest": "npm run lint",
    "prepublish": "npm run audit",
    "postmerge": "npm install"
  },
  "pre-commit": [
    "lint",
    "audit",
    "missing",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/martinmicunda/employee-scheduling-api"
  },
  "bugs": {
    "url": "https://github.com/martinmicunda/employee-scheduling-api/issues"
  },
  "files": [
    "lib",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "employee scheduling",
    "scheduler",
    "mm",
    "es6",
    "iojs"
  ],
  "dependencies": {
    "koa": "0.20.0",
    "mm-node-logger": "0.0.*",
    "couchbase": "2.0.8"
  },
  "devDependencies": {
    "nsp": "^1.0.0",
    "jshint": "^2.7.0",
    "nodemon": "1.3.7"
  },
  "license": {
    "type": "GNU GPL v3 license",
    "url": "https://github.com/martinmicunda/employee-scheduling-api/master/LICENSE"
  },
  "engines": {
    "node": ">=0.12",
    "npm": ">=2.x"
  }
}
