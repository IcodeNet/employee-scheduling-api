{
  "name": "employee-scheduling-api",
  "version": "0.0.0",
  "description": "An API component for Employee Scheduling application.",
  "author": {
    "name": "Martin Micunda",
    "url": "http://martinmicunda.com"
  },
  "main": "index.js",
  "scripts": {
    "start": "node --es_staging --harmony_arrow_functions -r babel/register index",
    "babel": "babel 'lib index.js' --out-dir dist --source-maps",
    "start-prod": "node ./dist/index",
    "start-babel": "babel-node --harmony index",
    "start-dev": "nodemon --watch 'lib/**/*' --watch 'index.js' --ext 'js json' --exec 'npm start'",
    "start-debug": "npm run start-dev -- --debug",
    "lint": "jshint lib/**/*.js",
    "clean": "rm -rvf dist",
    "audit": "nsp audit-shrinkwrap && nsp audit-package",
    "missing": "npm ls --depth 1",
    "outdated": "npm outdated --depth 0",
    "prepush": "npm shrinkwrap && npm test",
    "pretest": "npm run lint",
    "postmerge": "npm install"
  },
  "pre-commit": [
    "lint",
    "audit",
    "missing",
    "outdated"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/martinmicunda/employee-scheduling-api"
  },
  "bugs": {
    "url": "https://github.com/martinmicunda/employee-scheduling-api/issues"
  },
  "files": [
    "lib/",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "employee scheduling",
    "scheduler",
    "mm",
    "es6"
  ],
  "dependencies": {
    "koa": "0.21.0",
    "joi": "6.4.3",
    "koa-body": "1.2.1",
    "koa-cors": "0.0.14",
    "koa-mount": "1.3.0",
    "koa-error": "1.1.3",
    "koa-helmet": "0.1.0",
    "koa-logger": "1.2.2",
    "koa-joi-router": "1.3.1",
    "koa-response-time": "1.0.2",
    "koa-methodoverride": "0.4.2",
    "path": "^0.11.14",
    "glob": "^5.0.3",
    "lodash": "^3.5.0",
    "couchbase": "2.0.8",
    "node-uuid": "1.4.3",
    "mm-node-logger": "0.0.*"
  },
  "devDependencies": {
    "nsp": "^1.0.0",
    "jshint": "^2.7.0",
    "nodemon": "1.3.7",
    "babel": "5.5.3"
  },
  "license": "GPL-3.0",
  "engines": {
    "node": ">=0.12.x",
    "npm": ">=2.x"
  }
}
